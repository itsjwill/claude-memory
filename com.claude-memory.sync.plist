<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.claude-memory.sync</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/maskedhunter/coding/claude-memory/.venv/bin/python3</string>
